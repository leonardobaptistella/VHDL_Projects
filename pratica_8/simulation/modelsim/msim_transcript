add wave -position insertpoint  \
sim:/pratica_8/A
add wave -position insertpoint  \
sim:/pratica_8/B
add wave -position insertpoint  \
sim:/pratica_8/S
add wave -position insertpoint  \
sim:/pratica_8/carry
add wave -position insertpoint  \
sim:/pratica_8/C_O
add wave -position insertpoint  \
sim:/pratica_8/C
force -freeze sim:/pratica_8/A 1 0, 0 {50 ps} -r 100
# Value length (1) does not equal array index length (4).
# 
# ** Error: (vsim-4011) Invalid force value: 1 0, 0 {50 ps}.
