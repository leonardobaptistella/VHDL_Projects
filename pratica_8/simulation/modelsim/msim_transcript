run
run
force -freeze sim:/pratica_8/A(3) 1 0, 0 {50 ps} -r 100
force -freeze sim:/pratica_8/A(2) 1 0, 0 {50 ps} -r 100
force -freeze sim:/pratica_8/A(1) 1 0, 0 {50 ps} -r 100
force -freeze sim:/pratica_8/A(0) 1 0, 0 {50 ps} -r 100
force -freeze sim:/pratica_8/B(3) 0 0, 1 {50 ps} -r 100
force -freeze sim:/pratica_8/B(2) 0 0, 1 {50 ps} -r 100
force -freeze sim:/pratica_8/B(1) 0 0, 1 {50 ps} -r 100
force -freeze sim:/pratica_8/B(0) 1 0, 0 {50 ps} -r 100
run
run
